<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a2019_seg2105_project.ui.clinicApp.featuresAdmin.AdminServiceFragment"
    tools:ignore="MergeRootFrame"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_admin_service">

    <Button
        android:id="@+id/btn_admin_add_service"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center"
        android:text="@string/admin_addServices"
        />

    <Button
        android:id="@+id/btn_adminAccount_return"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:text="@string/admin_returnToPrevious" />

    <androidx.recyclerview.widget.RecyclerView
        android:id ="@+id/admin_service_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</FrameLayout>